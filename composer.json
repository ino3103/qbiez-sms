{
    "name": "qsms/qbiez-sms",
    "description": "Laravel package for Qbiez SMS API integration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ino Moses",
            "email": "ino.m@qbiez.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Qsms\\QbiezSms\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Qsms\\QbiezSms\\QbiezSmsServiceProvider"
            ],
            "aliases": {
                "Qsms": "Qsms\\QbiezSms\\Facades\\Qsms"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}